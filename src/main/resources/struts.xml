<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
	<constant name="struts.devMode" value="true"/>
	<package name="default" extends="struts-default" namespace="/">
		<action name="cardgame" class="prg.WelcomeAction">
			<result name="success">/WEB-INF/content/cardgame.jsp</result>
		</action>
		<action name="SessionAcction" class="prg.SessionAction" method="execute">
			<result name="success">/WEB-INF/content/Session.jsp</result>
		</action>
		<action name="cardgameChooseHigher" class="prg.WelcomeAction" method="chooseHigher">
			<result name="success">/WEB-INF/content/cardgame.jsp</result>
		</action>
		<action name="cardgameChooseLower" class="prg.WelcomeAction" method="chooseLower">
			<result name="success">/WEB-INF/content/cardgame.jsp</result>
		</action>
	</package>
</struts>